{
  "source": {
    "include": "lib/ical",
    "includePattern": ".js$"
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "encoding": "utf8",
    "readme": "README.md",
    "destination": "docs/api/",
    "template": "node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "default_theme": "light"
    }
  },
  "markdown": {
    "hardwrap": false,
    "idInHeadings": true
  }
}
